#import <Foundation/Foundation.h>

/**
 *  @brief 日期选择器对应数据项
 */
@interface PCDatePickerDO : NSObject

/**
 *  @brief ID
 */
@property (nonatomic, copy) NSString *strID;

/**
 *  @brief 已经选择的日期内容
 */
@property (nonatomic, copy) NSString *strText;

/**
 *  @brief 最小日期
 */
@property (nonatomic, copy) NSString *minDateString;

/**
 *  @brief 最大日期
 */
@property (nonatomic, copy) NSString *maxDateString;

/**
 *  @brief 日期格式
 */
@property (nonatomic, copy) NSString *format;

/**
 *  @brief 回调函数
 */
@property (nonatomic, copy) NSString *callbackString;

/**
 *  @brief 快速实例化
 *
 *  @param datePickerDict 字典
 *
 *  @return 实例化
 */
-(id)initWithDict:(NSDictionary*)datePickerDict;

/**
 *  @brief 判断当前日期类型
 *
 *  @return 3：年月日； 4：时分 其他：年月
 */
-(NSInteger)currentPickerType;

@end
