#import "JSProperty.h"

@implementation JSProperty

- (id)init {
    if (self = [super init]) {
//        self.tabBarController =
    }
    return self;
}

+ (instancetype)sharedApp {
    static JSProperty *app = nil;
    
    static dispatch_once_t onceToken;
    dispatch_once(&onceToken, ^{
        app = [[self alloc] init];
    });
    
    return app;
}

@end

#ifdef __cplusplus
extern "C" {
#endif
    JSProperty *App() {
        
        return [JSProperty sharedApp];
    }
#ifdef __cplusplus
}
#endif
