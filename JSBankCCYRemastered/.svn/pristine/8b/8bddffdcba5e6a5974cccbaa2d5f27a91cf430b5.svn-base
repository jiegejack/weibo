#import "RequestUrl.h"

@implementation RequestUrl

- (id)init {
    if (self = [super init]) {
        self.loginPassword = @"custCommon/loginByPassword.do";
        self.registerLogin = @"custCommon/register.do";
        self.biometryLogin = @"custCommon/loginbyFinger.do";
        self.SMSCodeLogin  = @"custCommon/loginByCode.do";
        self.SMSCode       = @"common/sendMsgCode.do";
        self.versionUpdate = @"app/appVerQry.do";
        self.placeholder = @"";
        self.tabBarIcon = @"informationCommon/getBottomMenuImage.do";
        self.setPassword = @"custCommon/setLoginPassword.do";
        self.faceRecord = @"custCommon/faceDetect.do";
        self.splashRequest = @"informationCommon/splashScreenQry.do";
    }
    return self;
}

+ (instancetype)sharedRequest {
    static RequestUrl *Request = nil;
    
    static dispatch_once_t onceToken;
    dispatch_once(&onceToken, ^{
        Request = [[self alloc] init];
    });
    
    return Request;
}

@end

#ifdef __cplusplus
extern "C" {
#endif
    RequestUrl *Request() {
        
        return [RequestUrl sharedRequest];
    }
#ifdef __cplusplus
}
#endif
