#import <Foundation/Foundation.h>

NS_ASSUME_NONNULL_BEGIN

@interface PCDWebCacheConfigMode : NSObject

/**
 * 缓存配置单例
 */
+(PCDWebCacheConfigMode *)shareInstance;

/**
 *  @brief 是否是调试模式
 *  YES 调试模式  NO：发布模式
 */
@property(nonatomic,readonly) BOOL debugTag;

/**
 *  @brief Info信息获取模式
 *
 *  NO 全量模式  YES 分段增量模式
 *
 */
@property(nonatomic,readonly) BOOL cacheDataType;

/**
 *  @brief 缓存模式
 *
 * NO:全量模式  YES :增量模式
 *
 */
@property(nonatomic,readonly) BOOL webCacheType;

/**
 *  @brief 是否支持无网络加载（无网络加载安全级别交低）
 *
 * NO:不支持  YES:支持
 *
 */
@property(nonatomic,readonly) BOOL loadDataType;


/**
 *  @brief 监听更新时间间隔
 *
 */
@property(nonatomic,readonly) NSInteger nTimeCount;


/**
 *  @brief 版本接口
 *
 */
@property(nonatomic,readonly) NSString *verUrl;

/**
 *  @brief 缓存列表接口
 *
 */
@property(nonatomic,readonly) NSString *infoUrl;

/**
 *  @brief 预制资源文件是否加密
 *
 * YES:加密  NO:不加密
 *
 */
@property(nonatomic,readonly) BOOL cacheEncryption;

/**
 *  @brief 秘钥报文
 *
 */
@property(nonatomic,readonly) NSString *strEncryData;

/**
 * @brief 预制文件名
 *
 */
@property(nonatomic,readonly) NSString *resourecName;

/**
 * @brief 初始化
 *
 */
-(void)initData;
@end

NS_ASSUME_NONNULL_END
