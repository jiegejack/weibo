/**
 *
 * PCCertificateStatusDefault: 默认
 * PCCertificateStatusNotValidate: 始终
 * PCCertificateStatusValidateCer: 指定信任证书
 *
 */
typedef NS_ENUM(NSInteger, PCHTTPCertificateStatus) {
    PCCertificateStatusDefault = 0,
    PCCertificateStatusNotValidate,
    PCCertificateStatusValidateCer,
};

/**
 加密规则
 
 - PCRequestEncryptNone: 不加密
 - PCRequestEncryptDefault: 三段式加密，默认
 - PCRequestEncryptSM: 国密三段式加密
 */
typedef NS_ENUM(NSInteger,PCDRequestEncryptType) {
    PCDRequestEncryptNone =0,
    PCDRequestEncryptDefault =1,
    PCDRequestEncryptSM =2,
    PCDRequestEncryptSMVer2 = 3
};

/**
 解密规则
 
 - PCRequestDecryptNone: 不解密
 - PCRequestDecryptDefault: AES解密，默认
 - PCRequestDecryptMix: 去填充解混淆AES解密
 - PCRequestDecryptSM4: SM4解密
 */
typedef NS_ENUM(NSInteger,PCDRequestDecryptType) {
    PCDRequestDecryptNone =0,
    PCDRequestDecryptDefault =1,
    PCDRequestDecryptMix =2,
    PCDRequestDecryptSM4 =3,
    PCDRequestDecryptSM4Ver2 = 4,
};


