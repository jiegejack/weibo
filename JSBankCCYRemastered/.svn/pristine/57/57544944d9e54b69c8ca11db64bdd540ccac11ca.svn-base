#import <Foundation/Foundation.h>

/**
 *  @brief web导航条左右按钮数据项
 */
@interface PCDWebNavigationItemDO : NSObject

/**
 *  @brief 是否存在按钮，部分项目不使用该字段
 */
@property(nonatomic,assign) BOOL isExist;

/**
 *  @brief 按钮标题
 */
@property(nullable,nonatomic,copy) NSString *itemTitle;

/**
 *  @brief 按钮对应JS函数
 */
@property(nullable,nonatomic,copy) NSString *functionName;

/**
 图标
 */
@property (nonatomic,copy) NSString *icon;

/**
 未知
 */
@property (nonatomic,assign) float sort;

/**
 *  @brief 初始化
 *
 *  @param itemDict 字典
 *
 *  @return 实例化
 */
-(nonnull id)initWithDict:(nullable NSDictionary *)itemDict;

@end

/**
 *  @brief 导航条web解析数据项
 */
@interface PCDWebNavigationDO : NSObject

/**
 *  @brief 导航条标题
 */
@property(nullable,nonatomic, copy)NSString *title;

/**
 *  @brief 左按钮数据项
 */
@property(nullable,nonatomic, strong)PCDWebNavigationItemDO *leftItemDO;

/**
 *  @brief 右按钮数据项
 */
@property(nullable,nonatomic, strong)PCDWebNavigationItemDO *rightItemDO;

/**
 *  @brief 初始化
 *
 *  @param navigationDict 字典
 *
 *  @return 实例化
 */
-(nonnull id)initWithDict:(nullable NSDictionary*)navigationDict;

@end
