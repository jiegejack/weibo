#import "PCDSessionIDManager.h"
#import <PCDBaseClass/PCDBaseClass.h>

@implementation PCDSessionIDManager

+ (instancetype)defaultManager{
    
    static PCDSessionIDManager *manager = nil;
    static dispatch_once_t onceToken;
    dispatch_once(&onceToken, ^{
        manager = [[PCDSessionIDManager alloc] init];
        
        manager.loginSIDs = [@{} mutableCopy];
        manager.gatewaySIDs = [@{} mutableCopy];
        manager.connectSIDs = [@{} mutableCopy];
        manager.loginUrls = [@[] mutableCopy];
        manager.channels = [@[[PCDUtil channelId]] mutableCopy];
    });
    return manager;
}

- (void)clearID{
    self.connectSIDs = [@{} mutableCopy];
    self.loginSIDs = [@{} mutableCopy];
}

@end
