#import "CheckLoginState.h"

@implementation CheckLoginState

- (void)handler:(id)data andContext:(PCDContext *)context ResponseCallback:(WVJBResponseCallback )responseCallback {
    NSDictionary *dict = (NSDictionary *)data;
    NSString *callback =[dict objectForKey:@"callback"];
    callback = callback.notNull ? callback : @"";
    BOOL isLogin = [JSLoginManager clientIsLogin];
    evaluateJavaScriptOnMainThread(self, callback, isLogin ? @"1" : @"0");
}
    
@end
