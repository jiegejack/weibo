#import "DataCache.h"

@implementation DataCache

+ (void)setCacheString:(NSString *)string forkey:(NSString *)key {
    if (!key.notNull) {
        return;
    }
    PCDDataStorageManger *manager = [[PCDDataStorageManger alloc] init];
    manager.Aeskey = key;
    [manager setCacheUDString:string forKey:key];
}

+ (NSString *)getCacheStringForkey:(NSString *)key {
    if (!key.notNull) {
        return @"";
    }
    PCDDataStorageManger *manager = [[PCDDataStorageManger alloc] init];
    manager.Aeskey = key;
    NSString *result = [manager cacheUDStringForKey:key];
    
    return result;
}

@end
