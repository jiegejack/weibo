#import "CommonColor.h"

@implementation CommonColor

- (id)init {
    if (self = [super init]) {
        self.MainBlue = [UIColor colorFormString:@"#4285FE"];
        self.MainLight = [UIColor colorFormString:@"#999999"];
        self.White = [UIColor colorFormString:@"#FFFFFF"];
        self.Auxiliary = [UIColor colorFormString:@"#CCCCCC"];
        self.Black = [UIColor colorFormString:@"#000000"];
        self.MainBlack = [UIColor colorFormString:@"#666666"];
    }
    return self;
}

+ (instancetype)sharedColor {
    static CommonColor *commonColor = nil;
    
    static dispatch_once_t onceToken;
    dispatch_once(&onceToken, ^{
        commonColor = [[self alloc] init];
    });
    
    return commonColor;
}

#ifdef __cplusplus
extern "C" {
#endif
    CommonColor *Color() {
        
        return [CommonColor sharedColor];
    }
#ifdef __cplusplus
}
#endif

@end
