#import <Foundation/Foundation.h>

/**
 *  @brief  NSData扩展
 */
@interface NSData (Additions)

/**
 *  @brief  NSData对应的md5(大写)
 *
 *  @return md5
 */
- (NSString *)md5UpString;

/**
 *  @brief  NSData对应的md5(小写)
 *
 *  @return md5
 */
- (NSString *)md5LowString;

/**
 *  @brief  NSData对应的sha1序列化
 *
 *  @return sha1序列化
 */
- (NSString *)sha1String;

/**
 *  sha1已被google宣布攻破，建议使用sha256（后续版本将废弃该api）
 *  @brief  NSData对应的sha256序列化
 *
 *  @return sha256序列化
 */
- (NSString *)sha256String;

@end
