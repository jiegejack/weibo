#import <Foundation/Foundation.h>
#import <UIKit/UIKit.h>

/**
 *  @brief 自定义交互webView
 */
@interface PCDWebView : UIWebView

/**
 *  @brief 捕捉交互事件函数名，默认Fw.device.Device.getEvent()，可自定义
 */
@property(nullable,nonatomic,copy) NSString *getEventName;

/**
 *  @brief 开始交互
 */
-(void)startInteraction;

/**
 *  @brief 结束交互
 */
-(void)endInteraction;

/**
 *  @brief 添加交互事件
 *
 *  @param target   目标源
 *  @param action   动作函数
 *  @param eventCode 事件编号
 */
- (void)addTarget:(nullable id)target action:(nullable SEL)action forCode:(nonnull NSString *)eventCode;

/**
 *  @brief 添加一组交互事件，动作函数为'handCode'+'code'格式
 *
 *  @param target    目标源
 *  @param eventCodes 事件编号数组
 */
- (void)addTarget:(nullable id)target forCodes:(nonnull NSArray *)eventCodes;

@end
