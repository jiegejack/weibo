#import <Foundation/Foundation.h>

/**
 会话ID管理类
 */
@interface PCDSessionIDManager : NSObject

/**
 网关ID
 */
@property (nonatomic,copy) NSString *gatewaySID;

/**
 校验图形码用到
 */
@property (nonatomic,copy) NSString *connectSID;

/**
 登录会话ID
 */
@property (nonatomic,copy) NSString *loginSID;

/**
 可能有多个登录的sid
 */
@property (nonatomic,strong)  NSMutableDictionary *loginSIDs;

/**
 可能有多个登录前无回话请求用到的sid
 */
@property (nonatomic,strong) NSMutableDictionary *gatewaySIDs;

/**
 多个渠道的时候 可能需要多次创建会话 对应多个创建会话获取的sid
 */
@property (nonatomic,strong) NSMutableDictionary *connectSIDs;;

/**
 登录到其他功能 一个app可能需要登录多次
 */
@property (nonatomic,strong) NSMutableArray *loginUrls;

/**
 登录接口
 */
@property (nonatomic,copy)  NSString *loginUrl;

/**
 会话请求接口
 */
@property (nonatomic,copy) NSString *getConnectUrl;

/**
 所有渠道
 */
@property (nonatomic,strong) NSMutableArray *channels;

/**
 当前是哪个渠道在发请求
 */
@property (nonatomic,copy) NSString *currentChannel;


+ (instancetype)defaultManager;


/**
 清理回话ID
 */
- (void)clearID;

@end
