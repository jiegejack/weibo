#ifndef OBU_JYObuError_h
#define OBU_JYObuError_h
/* OBU协议状态码 */
#define STATUS_FAIL         0x01 //操作失败
#define STATUS_TIMEOUT      0x02 //等待回应超时
#define STATUS_BAKERR       0x03 //回应包数据错误
#define STATUS_DES          0x04 //DES计算错误
#define STATUS_MAC          0x05 //MAC错误
#define STATUS_DATALEN      0x06 //返回数据错误
#define STATUS_TRANSMIT_NOW 0x07 //正在发送指令

#define STATUS_FAIL_DESC         @"操作失败。"
#define STATUS_TIMEOUT_DESC      @"等待回应超时。"
#define STATUS_BAKERR_DESC       @"返回不正常包。"
#define STATUS_DES_DESC          @"DES计算错误。"
#define STATUS_MAC_DESC          @"计算MAC错误。"
#define STATUS_DATALEN_DESC      @"返回数据错误。"
#define STATUS_TRANSMIT_NOW_DESC @"正在发送指令，发送通道阻塞。"


#define STATUS_OK               0x00  // operating success
#define PICC_NOTAGERR           0xFE  // no card or PICC response timeout 无卡或卡无回应
#define PICC_CRCERR             0xFD  // CRC check error CRC错误
#define PICC_AUTHERR            0xFC  // generic authentication error 认证错误
#define PICC_PARITYERR          0xFB  // odd/even check error 奇偶校验错误
#define PICC_CODEERR            0xFA  // when writing or reading,information from card 正在读卡或者写卡中
#define PICC_SERNRERR           0xF9  // after anticollision process,get a invalid card serial numbers 无效序列号
#define PICC_KEYERR             0xF8  // the key that had loaded into the eeprom has a error format 写入EEPROM的密钥格式错误
#define PICC_NOTAUTHERR         0xF7  // invalid operation before authentication 认证前的无效操作
#define PICC_BITCOUNTERR        0xF6  // received bits not equal to expectation 收到的位数错误
#define PICC_BYTECOUNTERR       0xF5  // received bytes not equal to expectation 收到字节错误
#define PICC_FRAMINGERR         0xF4  // frame error 帧错误
#define PICC_UNKNOWN_COMMAND    0xF3  // Reader IC command invalid,invalid PC command 无效指令
#define PICC_COLLERR            0xF2  // collision happened IC卡防冲突位异常
#define PICC_ACCESSTIMEOUT      0xF1  // Reader IC (FM1702_CE) timeout 读IC卡超时
#define PICC_NOT_IMPLEMENTED    0xF0  // Reader IC not implement the command 读卡器无法执行此指令。
#define PICC_WRONG_VALUE        0xEF  // invalid parameter 无效参数。
#define PICC_VALERR             0xEE  // electronic notecase operation using invalid value 使用电子钱包的操作无效值
#define PICC_WRITEERR           0xED  // write card error 写卡错误
#define PICC_OVFLERR            0xEC  // Reader IC (FM1702_CE) FIFO overflow  FIFO溢出
#define BLE_NOTAUTHERR          0xA1  // Reader not authentication 未设备认证

#define STATUS_OK_DESC               @"操作成功"
#define PICC_NOTAGERR_DESC           @"无卡或卡无回应。"
#define PICC_CRCERR_DESC             @"CRC错误。"
#define PICC_AUTHERR_DESC            @"认证错误。"
#define PICC_PARITYERR_DESC          @"奇偶校验错误。"
#define PICC_CODEERR_DESC            @"正在读卡或者写卡中。"
