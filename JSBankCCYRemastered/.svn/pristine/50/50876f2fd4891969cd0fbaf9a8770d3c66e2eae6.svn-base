/**
 不带单引号
 
 @param self self description
 @param callback callback description
 @param strResponse strResponse description
 @return return value description
 */
#define evaluateJavaScriptOnMainThread(self,callback,strResponse) UIWebView *jsWebView = self.bsContext.webView;\
[jsWebView performSelectorOnMainThread:@selector(stringByEvaluatingJavaScriptFromString:) withObject:[NSString stringWithFormat:@"%@(%@)",callback,strResponse] waitUntilDone:NO];


/**
 带单引号
 
 @param self self description
 @param callback callback description
 @param strResponse strResponse description
 @return return value description
 */
#define evaluateJavaScriptOnMainThread_quotes(self,callback,strResponse) UIWebView *jsWebView = self.bsContext.webView;\
[jsWebView performSelectorOnMainThread:@selector(stringByEvaluatingJavaScriptFromString:) withObject:[NSString stringWithFormat:@"%@('%@')",callback,strResponse] waitUntilDone:NO];

typedef void (^WVJBResponseCallback)(id responseData);
typedef void (^WVJBHandler)(id data, WVJBResponseCallback responseCallback,id tager,NSString *callName);

