#import "ExtensionTextField.h"

@implementation ExtensionTextField

- (CGRect)textRectForBounds:(CGRect)bounds {
    CGFloat deafaltInset = 30;
    return CGRectMake(bounds.origin.x, bounds.origin.y, bounds.size.width - self.inset - deafaltInset, bounds.size.height);
}

@end
