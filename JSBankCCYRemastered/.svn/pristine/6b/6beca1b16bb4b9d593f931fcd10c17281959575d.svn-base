#import "SessionTimeoutH5Plugin.h"
//#import "PlatformLoginViewController.h"

@implementation SessionTimeoutH5Plugin

- (void)handler:(id)data andContext:(PCDContext *)context ResponseCallback:(WVJBResponseCallback )responseCallback{
    
    if (![PCDUtil loginSession]) {
        JSBaseWebViewController *webVC = (JSBaseWebViewController *)context.viewController;

        [JSDeftAlert showAlert:@"会话超时,请重新登录" doneTitle:@"确定" cancelTitle:nil doneHandle:^{
            [JSLoginManager clientLoginWithTarget:webVC success:nil faild:nil];
        } cancelHandle:nil];
    }
}

@end
