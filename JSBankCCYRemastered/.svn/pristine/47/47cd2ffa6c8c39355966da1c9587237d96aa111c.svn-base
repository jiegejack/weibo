#import <Foundation/Foundation.h>

@interface NSString (JSUtils)

/**
 *  判断字符串是否为: 空字符串 或 nil/NULL
 *
 *  @return BOOL值
 */
- (BOOL)notNull;


/**
 *  判断包含字符串string
 *
 *  @return BOOL值
 */
- (BOOL)isContainString:(NSString *)string;


/**
 *  字符串中数字
 *
 *  @return 所有数字的拼接
 */
- (BOOL)isPureNum;
- (BOOL)isPureLetter;
- (BOOL)isMixture;

- (BOOL)isMobileNO;

@end
