/*deubg URL service address*/
FOUNDATION_EXTERN NSString *_Nonnull const PCDDebugServiceURL;
/*deubg URL resoure address*/
FOUNDATION_EXTERN NSString *_Nonnull const PCDDebugResoureURL;
/*preRelease URL service address*/
FOUNDATION_EXTERN NSString *_Nonnull const PCDPreReleaseServiceURL;
/*preRelease URL resoure address*/
FOUNDATION_EXTERN NSString *_Nonnull const PCDPreReleaseResoureURL;
/*release URL service address*/
FOUNDATION_EXTERN NSString *_Nonnull const PCDReleaseServiceURL;
/*release URL resoure address*/
FOUNDATION_EXTERN NSString *_Nonnull const PCDReleaseResoureURL;
/*current URL service address*/
FOUNDATION_EXTERN NSString *_Nonnull const PCDCurrentServiceURL;
/*current URL resoure address*/
FOUNDATION_EXTERN NSString *_Nonnull const PCDCurrentResoureURL;
/*loacl cache URL's AES key*/
FOUNDATION_EXTERN NSString *_Nonnull const PCDIPSettingAESKey;
/*RSA cer file name*/
FOUNDATION_EXTERN NSString *_Nonnull const PCDRSAFileNameString;
/*AES Offset of send Request*/
FOUNDATION_EXTERN NSString *_Nonnull const PCDRequestAESOffsetString;
/*AES Offset of response Request*/
FOUNDATION_EXTERN NSString *_Nonnull const PCDResponseAESOffsetString;
/*RSA cer file name*/
FOUNDATION_EXTERN NSString *_Nonnull const PCDRequestSMXString;
/*AES Offset of send Request*/
FOUNDATION_EXTERN NSString *_Nonnull const PCDRequestSMYString;
/*AES Offset of response Request*/
FOUNDATION_EXTERN NSString *_Nonnull const PCDRequestSMTagString;
/*Channel ID*/
FOUNDATION_EXTERN NSString *_Nonnull const PCDCurrentRequestChannelId;
/*是否使用网关*/
FOUNDATION_EXTERN NSString *_Nonnull const PCDCurrentUseGateway;
/**
 *  @brief 单例，PCKit私有类，工程外部请使用PCDUtil的setSingletonObject:forKey:和singletonObjectForKey:进行单例级数据存取
 */
@interface PCDSingleton : NSObject
/**
 *  @brief 单例化
 *
 *  @return 单例话对象
 */
+(nonnull PCDSingleton *)shareInstance;
/**
 *  @brief 登录session
 */
@property(nullable,nonatomic,strong) id loginSession;
/**
 *  @brief 是否是发布版本
 */
@property(nonatomic,assign) BOOL releaseFlag;
/**
 是否使用网关
 */
@property (nonatomic,assign) BOOL useGateWay;
/**
 *  @brief 是否输出日志
 */
@property(nonatomic,assign) BOOL logFlag;
/**
 *  @brief 是否使用webCache
 */
@property(nonatomic,assign) BOOL webCacheFlag;

/**
 *  @brief 渠道类型
 * MB:个人手机
 * NB:个人网银
 * WB:微信银行
 * DB:直销银行
 * CB:柜面
 */
@property(nonatomic,strong) NSString *chancelType;

/**
 *  @brief 法人编号
 */
@property(nonatomic,strong) NSString *incorpNo;


/**
 *  @brief 请求服务地址
 */
@property(nullable,nonatomic,strong) NSString *serviceAddress;
/**
 *  @brief 请求公共服务地址
 */
@property(nullable,nonatomic,strong) NSString *resourceServiceAddress;

/**
 渠道ID
 */
@property (nonatomic,copy) NSString *channelID;
/**
 *  @brief 自定义单例存储数据
 *
 *  @param object 对象
 *  @param key    键值
 */
-(void)setSingletonObject:(nullable id)object forKey:(nonnull NSString *)key;
/**
 *  @brief 获取单例存储数据
 *
 *  @param key 键值
 *
 *  @return 存储对象
 */
-(nullable id)singletonObjectForKey:(nonnull NSString *)key;
/**
 *  @brief 初始化App参数
 *
 *  @param dict 参数字典
 */
+(void)initAppSettingWithDictionary:(nonnull NSDictionary *)dict;

@end
