#import <Foundation/Foundation.h>

@class PCDWebViewMonitor;

@protocol SYWebViewMonitorDelegate <NSObject>

@optional

- (void)webViewMonitor:(PCDWebViewMonitor *)webViewMonitor uploadMonitorDataForURL:(NSString *)urlString loadTime:(NSTimeInterval)loadTime;

@end

@interface PCDWebViewMonitor : NSObject

@property (nonatomic,copy) void (^monitorBlock)(NSString *urlString,NSTimeInterval loadTime);

@property (nonatomic, weak) id<SYWebViewMonitorDelegate> delegate;

+ (instancetype)sharedInstance;

- (void)startMonitor;

- (void)stopMonitor;

@end
