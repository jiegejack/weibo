#import "APPConfig.h"

@implementation APPConfig

- (id)init {
    if (self = [super init]) {
        self.baidu_appKey = @"3a42ed3e5c8c94dafe97b2e43d0e758c";
        
        NSString *bd = [[[NSBundle mainBundle] infoDictionary] objectForKey:@"CFBundleIdentifier"];
        NSString *enterprise = @"";
        NSString *company = @"";
        self.licence_kb = [bd hasPrefix:@"com.yitong"] ? enterprise : company;
    }
    return self;
}

+ (instancetype)sharedApp {
    static APPConfig *Config = nil;
    
    static dispatch_once_t onceToken;
    dispatch_once(&onceToken, ^{
        Config = [[self alloc] init];
    });
    
    return Config;
}

@end

#ifdef __cplusplus
extern "C" {
#endif
    APPConfig *Config() {
        
        return [APPConfig sharedApp];
    }
#ifdef __cplusplus
}
#endif
